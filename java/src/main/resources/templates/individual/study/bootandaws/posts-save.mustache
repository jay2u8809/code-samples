{{>individual/study/bootandaws/layout/header}}
<h1> Register Posts </h1>
<div class="col-md-12">
  <div class="col-md-4">
    <form>
      <div class="form-group">
        <label for="title">Title</label>
        <input type="text" class="form-control" id="title" placeholder="Input Title" />
      </div>
      <div class="form-group">
        <label for="author">Author</label>
        <input type="text" class="form-control" id="author" placeholder="Input Author" />
      </div>
      <div class="form-group">
        <label for="content">Content</label>
        <textarea class="form-control" id="title" placeholder="Input Content"></textarea>
      </div>
    </form>
    <a href="/" role="button" class="btn btn-secondary">Cancel</a>
    <button type="button" class="btn btn-primary" id="btn-save">Register</button>
  </div>
</div>
{{>individual/study/bootandaws/layout/footer}}